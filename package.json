{
  "name": "yojana",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "test": "jest --watch",
    "test:e2e": "yarn docker:up && dotenv -e .env.test yarn prisma migrate reset -- --force && dotenv -e .env.test start-server-and-test dev http://localhost:3000/hello \"cypress open --e2e\"",
    "test:e2e:headless": "yarn docker:up && dotenv -e .env.test yarn prisma migrate reset -- --force && dotenv -e .env.test start-server-and-test dev http://localhost:3000/hello \"cypress run --e2e --config video=false\"",
    "test:coverage": "jest --coverage",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "prisma:seed": "prisma db seed",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d && delay 10",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@hookform/resolvers": "^3.0.0",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@prisma/client": "^4.11.0",
    "@reduxjs/toolkit": "^1.9.3",
    "cookies-next": "^2.1.1",
    "cron-parser": "^4.8.1",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.7",
    "ical-generator": "^3.6.1",
    "lodash": "^4.17.21",
    "next": "^13.2.4",
    "next-auth": "^4.20.1",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18.2.0",
    "react-bootstrap-icons": "^1.10.2",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.7",
    "react-nanny": "^2.15.0",
    "react-redux": "^8.0.5",
    "react-to-print": "2.14.12",
    "wcag-contrast": "^3.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^2.26.0",
    "@next/bundle-analyzer": "^13.2.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/cypress-dotenv": "^2.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.11",
    "@types/wcag-contrast": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.8.1",
    "cypress-dotenv": "^2.0.0",
    "delay-cli": "^2.0.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-extended": "^3.0.3",
    "lint-staged": "^13.2.0",
    "node-mocks-http": "^1.12.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.6",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "prisma": "^4.11.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,json,yml,yaml}": "prettier --write"
  }
}
